<collapse-panel-click-area ($click)="toggle()">
    <i class="fa {{#if expanded}}fa-caret-down{{else}}fa-caret-right{{/if}}"></i>
    {{#if titleIcon}}<i class="fa title-icon {{titleIcon}}"></i>{{/if}}
    <span class="title-text">{{titleText}}</span>
</collapse-panel-click-area>
<content/>
